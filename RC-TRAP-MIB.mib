    rcTlsTdiEndptTdId, rcTlsTdiEndptUniAddr, rcTlsEndptWatermarkIndication,
    rcTlsTestTdcTestId, rcTlsTdcTestResultIndication,
    rcTlsUniIpServiceWatermarkIndication, rcTlsTdcTestRttExceedIndication
