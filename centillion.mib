    mpcMpsAtmAddr AtmAddr
    }

mpcMpsIndex OBJECT-TYPE
    SYNTAX MpsIndex
--    Rsyntax Integer32(1..2147483647)
--  ACCESS not-accessible
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The MPS's index which is used to identify
