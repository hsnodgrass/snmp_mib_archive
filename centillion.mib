        "ATM Forum PNNI 1.0 Section 5.8.1.3"
    INDEX { pnniNodeIndex, pnniRouteAddrAddress, pnniRouteAddrPrefixLength, pnniRouteAddrIndex }
    ::= { pnniRouteAddrTable 1 }

