    pnniRouteAddrAddress AtmAddrPrefix,
    pnniRouteAddrPrefixLength PnniPrefixLength,
    pnniRouteAddrIndex Integer32,
    pnniRouteAddrIfIndex InterfaceIndex,
    pnniRouteAddrAdvertisingNodeId PnniNodeId,
