    	::= { adFRPerformDayPortStatusEntry 18 }

adFRPerformDayPortAsyncStatusFrame OBJECT-TYPE
	SYNTAX  Counter
    	ACCESS  read-only
    	STATUS  mandatory
    	DESCRIPTION
	   "Number of single PVC status frames received."
    	::= { adFRPerformDayPortStatusEntry 19 }

